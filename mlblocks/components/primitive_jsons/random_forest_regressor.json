{
  "name": "rf_regressor",
  "class": "sklearn.ensemble.RandomForestRegressor",
  "fit": "fit",
  "produce": "predict",
  "hyperparameters": {
    "criterion": {
      "type": "string",
      "values": ["mse"]
    },
    "max_features": {
      "type": "float",
      "range": [0.1, 1.0]
    },
    "max_depth": {
      "type": "int",
      "range": [3, 20]
    },
    "min_samples_split": {
      "type": "int",
      "range": [2, 4]
    },
    "min_samples_leaf": {
      "type": "int",
      "range": [1, 3]
    },
    "n_estimators": {
      "type": "int",
      "values": [10, 500]
    },
    "n_jobs": {
      "type": "int_cat",
      "values": [-1]
    }
  },
  "root_hyperparameters": ["criterion", "max_features", "max_depth", "min_samples_leaf", "min_samples_leaf", "n_estimators", "n_jobs"],
  "conditional_hyperparameters": {}
}
